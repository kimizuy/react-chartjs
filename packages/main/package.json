{
  "name": "@kimizuy/react-chartjs",
  "version": "0.1.0",
  "repository": "https://github.com/kimizuy/react-chartjs.git",
  "author": "kimizuy <kimizuyamasaki@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/react-chartjs.umd.js",
  "module": "./dist/react-chartjs.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-chartjs.es.js",
      "require": "./dist/react-chartjs.umd.js"
    }
  },
  "scripts": {
    "build": "vite build && yarn build:type",
    "build:type": "tsc --declaration --jsx react --esModuleInterop --skipLibCheck --outDir dist --emitDeclarationOnly src/index.tsx"
  },
  "dependencies": {
    "chart.js": "^3.5.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "use-deep-compare-effect": "^1.8.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  }
}
